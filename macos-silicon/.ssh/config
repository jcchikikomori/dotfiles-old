Host *
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host aws-ec2-sample
    HostName ec2-69-666-999-0.ap-southeast-1.compute.amazonaws.com
    User ubuntu
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host aws-bastion-sample
    HostName ec2-69-666-999-0.ap-southeast-1.compute.amazonaws.com
    User ubuntu
    IdentityFile ~/Documents/cert.pem
    ServerAliveInterval 60
    ServerAliveCountMax 10

